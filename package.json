{
  "name": "reload-shoot-block",
  "version": "1.0.0",
  "description": "A nodejs based multiplayer Western spin on the classic Rock, Paper, Scissors!",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "run": "tsc && npm run compile",
    "compile": "tsc --watch & node out/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigbeno37/ReloadShootBlock.git"
  },
  "keywords": [
    "rock",
    "paper",
    "scissors",
    "western",
    "multiplayer",
    "express",
    "sockets",
    "web",
    "socket"
  ],
  "author": "bigbeno37",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bigbeno37/ReloadShootBlock/issues"
  },
  "homepage": "https://bigbeno37.github.com/ReloadShootBlock",
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.6",
    "@types/bases": "^0.2.2",
    "@types/express": "^4.16.0",
    "@types/ws": "^5.1.2",
    "typescript": "^2.9.1"
  },
  "dependencies": {
    "bases": "^0.2.1",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "ws": "^5.2.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/out/"
    ]
  }
}
